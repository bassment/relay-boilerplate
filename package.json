{
  "name": "eliftech",
  "version": "0.0.1",
  "main": "server.js",
  "scripts": {
    "start": "nodemon --watch data/schema.js --watch server.js --watch package.json --exec babel-node server.js",
    "production": "better-npm-run build:start",
    "build": "rimraf dist && better-npm-run build",
    "eslint": "eslint .",
    "jscs": "jscs ."
  },
  "betterScripts": {
    "build": {
      "command": "webpack --config ./webpack.production.config.js --progress --profile --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:start": {
      "command": "node server",
      "env": {
        "NODE_ENV": "production",
        "PORT": "8000"
      }
    }
  },
  "dependencies": {
    "babel-cli": "^6.6.5",
    "basscss": "^7.1.1",
    "express": "^4.13.3",
    "express-graphql": "^0.4.10",
    "graphql": "^0.4.18",
    "mongodb": "^2.1.11",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-helmet": "^2.3.1",
    "react-relay": "^0.7.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "babel-core": "^6.3.26",
    "babel-eslint": "^4.1.6",
    "babel-jscs": "^2.0.5",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-relay-plugin": "^0.7.3",
    "better-npm-run": "0.0.5",
    "css-loader": "^0.19.0",
    "eslint": "^1.5.0",
    "eslint-plugin-react": "^3.4.2",
    "extract-text-webpack-plugin": "^0.8.2",
    "html-webpack-plugin": "^1.6.1",
    "jscs": "^2.1.1",
    "json-loader": "^0.5.3",
    "nodemon": "^1.9.1",
    "postcss-loader": "^0.6.0",
    "react-addons-test-utils": "^0.14.3",
    "react-transform-catch-errors": "^1.0.1",
    "react-transform-hmr": "^1.0.0",
    "redbox-react": "^1.2.0",
    "rimraf": "^2.4.3",
    "stats-webpack-plugin": "^0.2.1",
    "style-loader": "^0.12.4",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0"
  }
}
